{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",

    "description": "Population of Scotland 1855-2020",

    "title": {
        "text": "Population of Scotland 1855-2020",
        "subtitle": "Chart produced from CSV file (false observation added)",
        "subtitleFontStyle": "italic",
        "subtitleFontSize": 10,
        "anchor": "start",
        "color": "black"
    },

    "data": {
        "url": "https://raw.githubusercontent.com/on19193/on19193.github.io/main/ScotlandPopulation.csv"
    },
    "height": 300,
    "width": 300,
    "mark": {
        "type": "line",
        "point": false,
        "interpolate": "monotone",
        "color": "blueviolet"
    },
    "encoding": {
        "x": {
            "field": "Year ",
            "type": "temporal",
            "axis": {
                "title": "Year",
                "grid": false,
                "labelSeparation": 50,
                "ticks": false
            }
        },
        "y": {
            "field": "Population",
            "type": "quantitative",
            "title": "Population",
            "axis": { "grid": false }
        }
    }
}