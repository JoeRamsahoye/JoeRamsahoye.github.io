{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "config": {"background": "#e6edf0"},
    "title": {
      "text": "Tesla Share Price",
      "subtitle": "Price at close from 06/02/2021 to 06/08/2021 ($), Ticker: TSLA",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "anchor": "start",
      "color": "black"
    },
    "data": {
      "url": "https://www.alphavantage.co/query?function=TIME_SERIES_DAILY&symbol=TSLA&interval=5min&outputsize=full&apikey=S1DLPXISA0IMJP95&datatype=csv",
      "format": {"type": "csv"}
    },
    "height": 350,
    "width": 350,
    "mark": {
      "type": "area",
      "clip": true,
      "line": {"color": "#0d84b3"},
      "color": {
        "x1": 1,
        "y1": 1,
        "x2": 1,
        "y2": 0,
        "gradient": "linear",
        "stops": [
          {"offset": 0.2, "color": "#e6edf0"},
          {"offset": 1, "color": "#0d84b3"}
        ]
      }
    },
  
    "transform": [
      {
        "calculate": "month(datum.timestamp)",
        "as": "month_timestamp"
      },
      {"filter": "datum.month_timestamp>minMonth"},
      {"filter": "datum.month_timestamp<maxMonth"}
    ],
  
    "params": [
      {
        "name": "minMonth",
        "value": 0,
        "bind": {
          "input": "range",
          "min": 0,
          "max": 12,
          "step": 1,
          "name": "Start month:"
        }
      },
      {
        "name": "maxMonth",
        "value": 12,
        "bind": {
          "input": "range",
          "min": 0,
          "max": 12,
          "step": 1,
          "name": "End month:"
        }
      }
    ],
    "encoding": {
      "x": {
        "field": "timestamp",
        "type": "temporal",
        "title": "Date",
        "axis": {"grid": false},
        "scale": {
          "domainMin": {"year": 2021, "month": "feb", "date": 6},
          "domainMax": {"year": 2021, "month": "dec", "date": 25}
        }
      },
      "y": {
        "field": "close",
        "type": "quantitative",
        "title": "Price at close ($)",
        "axis": {"grid": false},
        "scale": {"domain": [500, 1400]}
      },
      "tooltip": [
        {"field": "timestamp", "title": "Date", "type": "temporal"},
        {"field": "close", "title": "Price at close ($)", "type": "quantitative"}
      ]
    }
  }